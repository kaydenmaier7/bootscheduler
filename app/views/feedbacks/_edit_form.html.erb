<div class="error_message">
<%= render 'partials/error_messages', active_object: @feedback %>
</div>

<%= form_for :feedback, url: boot_feedback_path(@feedback), method: :patch do |f| %>

    <%= f.radio_button :rating, 1 %>
    <%= f.label :rating, 1 %>
    <br>
    <%= f.radio_button :rating, 2 %>
    <%= f.label :rating, 2 %>
    <br>
    <%= f.radio_button :rating, 3 %>
    <%= f.label :rating, 3 %>
    <br>
    <%= f.radio_button :rating, 4 %>
    <%= f.label :rating, 4 %>
    <br>
    <%= f.radio_button :rating, 5 %>
    <%= f.label :rating, 5 %>
    <br>
    <br>
    <%= f.label :comment %>:
    <%= f.text_area :comment %>
    <br>
    <br>
    <%= f.submit %>
<% end %>